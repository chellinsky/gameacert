<%= simple_form_for(@space) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :topic %>
    <%= f.input :detail %>
    <%= f.input :gameid %>
    <%= f.input :gameorder %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
